{
  "name": "Output",
  "plural": "Outputs",
  "base": "ValueHolder",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "connected": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "relatedCore": {
      "type": "belongsTo",
      "model": "Core",
      "foreignKey": "id"
    },
    "relatedSinks": {
      "type": "hasMany",
      "model": "Sink",
      "foreignKey": "outputId"
    }
  },
  "acls": [],
  "methods": {
    "prototype.disconnect": {
      "accepts": [
        {
          "arg": "sinkIdList",
          "type": "array",
          "required": true,
          "description": "List of Sinks Ids to be disconnected",
          "http": {
            "source": "form"
          }
        }
      ],
      "returns": [],
      "description": "Disconnect Output from Sinks",
      "http": [
        {
          "path": "/disconnect",
          "verb": "post"
        },
        {
          "path": "/disconnect",
          "verb": "put"
        }
      ]
    }
  }
}
